<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nsz.kotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".storage.ScopedStorageActivity" />

        <activity android:name=".thread.LaunchScopeActivity" />

        <activity android:name=".spannable.SpannableStringActivity" />

        <activity android:name=".aac.AndroidArchitectureComponentsActivity" />
        <activity android:name=".aac.live.data.LiveDataActivity" />
        <activity android:name=".aac.data.binding.DataBindingActivity" />
        <activity android:name=".aac.data.binding.DataBindingRecyclerViewActivity" />

    </application>

</manifest>